    "import yfinance as yf\n",
    "import pandas as pd\n",
    "\n",
    "# Fetch historical data for Nifty 50\n",
    "nifty50 = yf.download('^NSEI', start='1973-01-01', end='2025-03-24')\n",
    "print(nifty50.head())\n",
    "\n",
    "# Save to CSV if needed\n",
    "nifty50.to_csv('nifty50_historical_data.csv')\n"
